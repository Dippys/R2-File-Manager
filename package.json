  "version": "1.0.0",
  "description": "Cloudflare R2 File Manager with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["r2", "cloudflare", "file-manager", "s3"],
